<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_color_white"
    android:orientation="vertical"
    android:padding="@dimen/five">

    <TextView
        android:id="@+id/zheng_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/five"
        android:text="新增销售资料\n(征信)"
        android:textColor="@color/black"
        android:textSize="@dimen/text_fourteen" />

    <TextView
        android:id="@+id/rent_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/five"
        android:text="新增租赁资料"
        android:textColor="@color/black"
        android:textSize="@dimen/text_fourteen" />


    <TextView
        android:id="@+id/order_zheng_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/five"
        android:text="待审核订单"
        android:textColor="@color/black"
        android:textSize="@dimen/text_fourteen" />

    <TextView
        android:id="@+id/order_wan_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/five"
        android:text="待完善订单"
        android:textColor="@color/black"
        android:textSize="@dimen/text_fourteen" />

    <TextView
        android:id="@+id/customs_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/five"
        android:text="客户记录"
        android:textColor="@color/black"
        android:textSize="@dimen/text_fourteen" />

    <TextView
        android:id="@+id/orders_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/five"
        android:text="订单记录"
        android:textColor="@color/black"
        android:textSize="@dimen/text_fourteen" />
</LinearLayout>